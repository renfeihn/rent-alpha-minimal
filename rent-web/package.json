{
  "name": "rent-web",
  "version": "1.0.0",
  "description": "rent-web application",
  "author": "Saltykov Dmitry",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --progress --colors --open",
    "build:dll": "rimraf build && webpack -p --config webpack.config.dll.js --progress --profile --colors",
    "build": "rimraf build/distr && webpack -p --config webpack.config.prod.local.js --progress --profile --colors",
    "build:heroku": "rimraf build/distr && webpack -p --config webpack.config.prod.heroku.js --progress --profile --colors",
    "build:perspektiva": "rimraf build/distr && webpack -p --config webpack.config.prod.perspektiva.js --progress --profile --colors",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-airbnb": "2.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "html-webpack-plugin": "2.28.0",
    "html-webpack-include-assets-plugin": "0.0.5",
    "jest": "20.0.4",
    "less": "2.7.2",
    "less-loader": "4.0.4",
    "css-loader": "0.28.4",
    "style-loader": "0.18.2",
    "url-loader": "0.5.8",
    "react-addons-test-utils": "15.5.1",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0",
    "copy-webpack-plugin": "4.0.1",
    "rimraf": "2.6.1",
    "redux-devtools": "3.4.0",
    "redux-devtools-dock-monitor": "1.1.2",
    "redux-devtools-log-monitor": "1.3.0",
    "enzyme": "2.8.2",
    "sinon": "2.3.2"
  },
  "dependencies": {
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-redux": "5.0.5",
    "react-intl": "2.3.0",
    "prop-types": "15.5.10",
    "intl": "1.2.5",
    "intl-locales-supported": "1.0.0",
    "axios": "0.16.2",
    "redux": "3.6.0",
    "redux-saga": "0.15.3",
    "antd": "2.11.1",
    "async-validator": "1.6.9",
    "moment": "2.18.1",
    "extend": "3.0.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js"
    ]
  }
}
