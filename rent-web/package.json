{
  "name": "rent-web",
  "version": "1.0.0",
  "description": "rent-web application",
  "author": "Saltykov Dmitry",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --progress --colors --open",
    "build:dll": "rimraf build && webpack -p --config webpack.config.dll.js --progress --profile --colors",
    "build": "rimraf build/distr && webpack -p --config webpack.config.prod.local.js --progress --profile --colors",
    "build:heroku": "rimraf build/distr && webpack -p --config webpack.config.prod.heroku.js --progress --profile --colors",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-airbnb": "2.4.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "4.13.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "html-webpack-plugin": "2.30.1",
    "html-webpack-include-assets-plugin": "1.0.2",
    "jest": "21.2.1",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "css-loader": "0.28.7",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "react-addons-test-utils": "15.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "copy-webpack-plugin": "4.2.3",
    "rimraf": "2.6.2",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.2",
    "redux-devtools-log-monitor": "1.4.0",
    "enzyme": "3.2.0",
    "sinon": "4.1.3"
  },
  "dependencies": {
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-redux": "5.0.6",
    "react-intl": "2.4.0",
    "prop-types": "15.5.10",
    "intl": "1.2.5",
    "intl-locales-supported": "1.0.0",
    "axios": "0.17.1",
    "redux": "3.7.2",
    "redux-saga": "0.16.0",
    "antd": "2.13.11",
    "moment": "2.19.4",
    "extend": "3.0.1",
    "nprogress": "0.2.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js"
    ]
  }
}
