    "webpack-dev-server": "2.4.2",
    "rimraf": "2.6.1"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-redux": "5.0.3",
    "react-intl": "2.2.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "intl": "1.2.5",
    "intl-locales-supported": "1.0.0",
    "axios": "0.16.0",
    "redux": "3.6.0",
    "redux-saga": "0.14.5",
    "redux-devtools": "3.3.2",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.2.0",
    "enzyme": "2.8.0",
